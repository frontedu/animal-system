    "@testing-library/user-event": "~7.2.1",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-scripts": "~3.4.4"
  },
  "devDependencies": {
    "cross-env": "~7.0.2",
    "postinstall": "~0.7.0",
    "sass": "~1.42.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
